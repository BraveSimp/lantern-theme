	"editor.tokenColorCustomizations": {
			"textMateRules": [
				/// MARKDOWN ///
				{
					"scope": "markup.inline.raw",
					"settings": {
						"foreground": "#f99666"
					}
				},
				// quotesComments,
				{
					"scope": "string.other.link.description.title.markdown",
					"settings": {
						"foreground": "#fad256"
					}
				},
				{
					"scope": "markup.italic.markdown",
					"settings": {
						"foreground": "#e4cbb3",
						"fontStyle": "italic"
					}
				},
				// #Headings, heading
				{
					"scope": "punctuation.definition.heading.markdown, entity.name.section.markdown",
					"settings": {
						"foreground": "#f99666",
						"fontStyle": "bold"
					}
				},
				{
					"scope": "markup.bold",
					"settings": {
						"foreground": "#e4cbb3",
						"fontStyle": "bold"
					}
				},
				{
					"scope": "string.other.link.title.markdown",
					"settings": {
						"foreground": "#e86420",
						"fontStyle": "bold"
					}
				},
				{
					"scope": "punctuation.definition.string.begin.markdown",
					"settings": {
						"foreground": "#e4cbb3"
					}
				},
				{
					"scope": "punctuation.definition.string.end.markdown",
					"settings": {
						"foreground": "#e4cbb3"
					}
				},
				{
					"scope": "punctuation.definition.list.begin.markdown",
					"settings": {
						"foreground": "#f99666"
					}
				},
				{
					"scope": "entity.name.tag.yaml",
					"settings": {
						"foreground": "#e35b15",
						"fontStyle": "bold"
					}
				},
