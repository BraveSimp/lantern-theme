				// },
				// brackets,
				{
					"scope": [
						"punctuation.definition.dictionary.end.json.comments",
						"punctuation.definition.dictionary.begin.json.comments"
					],
					"settings": {
						"foreground": "#e4cbb3"
					}
				},
				/// CSS ///
				// class, id
				{
					"scope": "entity.other.attribute-name.class.css, entity.other.attribute-name.id",
					"settings": {
						"foreground": "#e86420"
